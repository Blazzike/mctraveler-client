{
	"schemaVersion": 1,
	"id": "mctraveler-client",
	"version": "${version}",
	"name": "MCTraveler Client",
	"description": "The official client mod for the MCTraveler server.",
	"authors": [
		"iElmo"
	],
	"contact": {
		"homepage": "https://mctraveler.eu/",
		"sources": "https://github.com/Blazzike/mctraveler-client",
		"issues": "https://github.com/Blazzike/mctraveler-client/issues"
	},
	"license": "CC0-1.0",
	"icon": "assets/mctraveler-client/icon.png",
	"environment": "client",
	"entrypoints": {
		"main": [
			{
				"value": "eu.mctraveler.MCTravelerClient",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"mctraveler-client.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.5",
		"minecraft": ">=1.21",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}